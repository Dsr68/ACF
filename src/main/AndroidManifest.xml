<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.sospirangueiro"
    android:versionCode="1"
    android:versionName="1.0">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_monetization_on_24"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.SOSPirangueiro">
        <activity android:name=".view.Calculos"
            android:parentActivityName=".view.MainActivity"/>
        <activity android:name=".view.TelaEditar"
            android:parentActivityName=".view.Listas"/>
        <activity android:name=".view.Listas"
            android:parentActivityName=".view.MainActivity"/>
        <activity android:name=".view.DespesasDiarias"
            android:parentActivityName=".view.MainActivity"/>
        <activity
            android:name=".view.MainActivity"
            android:label="@string/app_name"
            tools:ignore="MissingClass">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.DespesasFixasActivy"
            android:parentActivityName=".view.GastosActivy"/>
        <activity android:name=".view.GastosActivy"
            android:parentActivityName=".view.MainActivity" />
        <activity android:name=".view.MetaActivy"
            android:parentActivityName=".view.GastosActivy"/>
    </application>

</manifest>